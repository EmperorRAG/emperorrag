{
	"compilerOptions": {
		"baseUrl": ".",
		"composite": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"importHelpers": true,
		"isolatedModules": true,
		"lib": ["es2022"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022",
		"customConditions": ["@emperorrag/source"],
		"paths": {
			"@prisma/client": ["packages/prisma/better-auth-db/prisma/generated/client"],
			"@emperorrag/better-auth-utilities": ["packages/better-auth-utilities/src/better-auth-utilities.ts"],
			"@emperorrag/better-auth-utilities/client": ["packages/better-auth-utilities/src/lib/client/index.ts"],
			"@emperorrag/better-auth-utilities/server": ["packages/better-auth-utilities/src/lib/server/index.ts"],
			"@emperorrag/better-auth-utilities/config": ["packages/better-auth-utilities/src/lib/config/index.ts"],
			"@emperorrag/better-auth-utilities/core": ["packages/better-auth-utilities/src/lib/core/index.ts"],
			"@emperorrag/better-auth-utilities/change-password/client": [
				"packages/better-auth-utilities/src/lib/core/email/client/change-password/changePassword.ts"
			],
			"@emperorrag/better-auth-utilities/request-password-reset/client": [
				"packages/better-auth-utilities/src/lib/core/email/client/request-password-reset/requestPasswordReset.ts"
			],
			"@emperorrag/better-auth-utilities/reset-password/client": [
				"packages/better-auth-utilities/src/lib/core/email/client/reset-password/resetPassword.ts"
			],
			"@emperorrag/better-auth-utilities/send-verification-email/client": [
				"packages/better-auth-utilities/src/lib/core/email/client/send-verification-email/sendVerificationEmail.ts"
			],
			"@emperorrag/better-auth-utilities/sign-in-email/client": ["packages/better-auth-utilities/src/lib/core/email/client/sign-in-email/signInEmail.ts"],
			"@emperorrag/better-auth-utilities/sign-out/client": ["packages/better-auth-utilities/src/lib/core/email/client/sign-out/signOut.ts"],
			"@emperorrag/better-auth-utilities/sign-up-email/client": ["packages/better-auth-utilities/src/lib/core/email/client/sign-up-email/signUpEmail.ts"],
			"@emperorrag/better-auth-utilities/link-social/client": ["packages/better-auth-utilities/src/lib/core/account/client/link-social/linkSocial.ts"],
			"@emperorrag/better-auth-utilities/list-accounts/client": [
				"packages/better-auth-utilities/src/lib/core/account/client/list-accounts/listAccounts.ts"
			],
			"@emperorrag/better-auth-utilities/unlink-account/client": [
				"packages/better-auth-utilities/src/lib/core/account/client/unlink-account/unlinkAccount.ts"
			],
			"@emperorrag/better-auth-utilities/get-session/client": ["packages/better-auth-utilities/src/lib/core/session/client/get-session/getSession.ts"],
			"@emperorrag/better-auth-utilities/update-user/client": ["packages/better-auth-utilities/src/lib/core/user/client/update-user/updateUser.ts"],
			"@emperorrag/better-auth-utilities/sign-in/client": ["packages/better-auth-utilities/src/lib/core/oauth/client/sign-in/signIn.ts"],
			"@emperorrag/utilities": ["packages/utilities/src/utilities.ts"],
			"@emperorrag/utilities/helper-functions": ["packages/utilities/src/lib/helper-functions/helperFunctions.ts"],
			"@emperorrag/utilities/types": ["packages/utilities/src/lib/types/types.ts"],
			"@emperorrag/utilities/object-utils": ["packages/utilities/src/lib/helper-functions/object.utils.ts"],
			"@emperorrag/components": ["packages/components/src/index.ts"],
			"@emperorrag/components/*": ["packages/components/src/*"],
			"@emperorrag/prisma-better-auth-db": ["packages/prisma/better-auth-db/src/index.ts"],
			"@emperorrag/prisma-better-auth-db/*": ["packages/prisma/better-auth-db/src/*"]
		}
	}
}
