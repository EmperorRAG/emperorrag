{
  "name": "@emperorrag/components",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean:dist": "npx rimraf dist",
    "clean:node_modules": "npx rimraf node_modules",
    "clean:out-tsc": "npx rimraf out-tsc",
    "clean:all": "npm run clean:dist && npm run clean:node_modules && npm run clean:out-tsc",
    "tsc:list-files-only": "tsc --project tsconfig.json --listFilesOnly > tsc-list-files-only.txt",
    "tsc:list-files-only:lib": "tsc --project tsconfig.lib.json --listFilesOnly > tsc-list-files-only-lib.txt",
    "tsc:list-files-only:spec": "tsc --project tsconfig.spec.json --listFilesOnly > tsc-list-files-only-spec.txt",
    "tsc:list-files-only:storybook": "tsc --project tsconfig.storybook.json --listFilesOnly > tsc-list-files-only-storybook.txt",
    "tsc:show-config": "tsc --project tsconfig.json --showConfig > tsc-show-config.txt",
    "tsc:show-config:lib": "tsc --project tsconfig.lib.json --showConfig > tsc-show-config-lib.txt",
    "tsc:show-config:spec": "tsc --project tsconfig.spec.json --showConfig > tsc-show-config-spec.txt",
    "tsc:show-config:storybook": "tsc --project tsconfig.storybook.json --showConfig > tsc-show-config-storybook.txt",
    "tsc:extended-diagnostics": "tsc --project tsconfig.json --extendedDiagnostics > tsc-extended-diagnostics.txt",
    "tsc:extended-diagnostics:lib": "tsc --project tsconfig.lib.json --extendedDiagnostics > tsc-extended-diagnostics-lib.txt",
    "tsc:extended-diagnostics:spec": "tsc --project tsconfig.spec.json --extendedDiagnostics > tsc-extended-diagnostics-spec.txt",
    "tsc:extended-diagnostics:storybook": "tsc --project tsconfig.storybook.json --extendedDiagnostics > tsc-extended-diagnostics-storybook.txt",
    "tsc:explain-files": "tsc --project tsconfig.json --explainFiles > tsc-explain-files.txt",
    "tsc:explain-files:lib": "tsc --project tsconfig.lib.json --explainFiles > tsc-explain-files-lib.txt",
    "tsc:explain-files:spec": "tsc --project tsconfig.spec.json --explainFiles > tsc-explain-files-spec.txt",
    "tsc:explain-files:storybook": "tsc --project tsconfig.storybook.json --explainFiles > tsc-explain-files-storybook.txt",
    "tsc:trace-resolution": "tsc --project tsconfig.json --traceResolution > tsc-trace-resolution.txt",
    "tsc:trace-resolution:lib": "tsc --project tsconfig.lib.json --traceResolution > tsc-trace-resolution-lib.txt",
    "tsc:trace-resolution:spec": "tsc --project tsconfig.spec.json --traceResolution > tsc-trace-resolution-spec.txt",
    "tsc:trace-resolution:storybook": "tsc --project tsconfig.storybook.json --traceResolution > tsc-trace-resolution-storybook.txt"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@emperorrag/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "@emperorrag/source": "./src/lib/*.ts",
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "default": "./dist/lib/*.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "name": "components",
    "tags": [
      "library components shared front-end"
    ]
  },
  "dependencies": {
    "effect": "^3.18.4",
    "react": "19.0.0"
  },
  "devDependencies": {
    "tslib": "^2.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vite": "^7.0.0",
    "vitest": "^3.0.0",
    "@nx/vite": "21.6.5",
    "@testing-library/react": "16.1.0",
    "storybook": "10.1.2",
    "@storybook/react-vite": "10.1.2",
    "@vitejs/plugin-react-swc": "^3.5.0"
  }
}