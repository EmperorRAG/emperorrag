{
	"name": "@emperorrag/better-auth-utilities",
	"version": "0.0.1",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/better-auth-utilities.js",
	"module": "./dist/better-auth-utilities.js",
	"types": "./dist/better-auth-utilities.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"@emperorrag/source": "./src/better-auth-utilities.ts",
			"types": "./dist/better-auth-utilities.d.ts",
			"import": "./dist/better-auth-utilities.js",
			"default": "./dist/better-auth-utilities.js"
		},
		"./client": {
			"@emperorrag/source": "./src/lib/client/index.ts",
			"types": "./dist/lib/client/index.d.ts",
			"import": "./dist/lib/client/index.js",
			"default": "./dist/lib/client/index.js"
		},
		"./server": {
			"@emperorrag/source": "./src/lib/server/index.ts",
			"types": "./dist/lib/server/index.d.ts",
			"import": "./dist/lib/server/index.js",
			"default": "./dist/lib/server/index.js"
		},
		"./config": {
			"@emperorrag/source": "./src/lib/config/index.ts",
			"types": "./dist/lib/config/index.d.ts",
			"import": "./dist/lib/config/index.js",
			"default": "./dist/lib/config/index.js"
		},
		"./core": {
			"@emperorrag/source": "./src/lib/core/index.ts",
			"types": "./dist/lib/core/index.d.ts",
			"import": "./dist/lib/core/index.js",
			"default": "./dist/lib/core/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"!**/*.tsbuildinfo",
		"!**/*.test.*",
		"!**/*.spec.*"
	],
	"nx": {
		"name": "better-auth-utilities",
		"tags": [
			"utilities",
			"library",
			"common",
			"shared",
			"packages",
			"typescript",
			"better-auth"
		]
	},
	"dependencies": {
		"effect": "^3.18.4",
		"better-auth": "^1.3.27",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"tslib": "^2.3.0",
		"vite-plugin-dts": "^4.5.4",
		"vite": "^7.0.0",
		"vitest": "^3.0.0",
		"@nx/vite": "21.6.5"
	}
}
