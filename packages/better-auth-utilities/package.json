{
	"name": "@emperorrag/better-auth-utilities",
	"version": "0.0.1",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"@emperorrag/source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/lib/client.d.ts",
			"import": "./dist/lib/client.js",
			"default": "./dist/lib/client.js",
			"@emperorrag/source": "./src/lib/client.ts"
		},
		"./server": {
			"types": "./dist/lib/server.d.ts",
			"import": "./dist/lib/server.js",
			"default": "./dist/lib/server.js",
			"@emperorrag/source": "./src/lib/server.ts"
		},
		"./config": {
			"types": "./dist/lib/config.d.ts",
			"import": "./dist/lib/config.js",
			"default": "./dist/lib/config.js",
			"@emperorrag/source": "./src/lib/config.ts"
		}
	},
	"files": [
		"dist",
		"!**/*.tsbuildinfo"
	],
	"nx": {
		"name": "better-auth-utilities",
		"tags": [
			"utilities",
			"library",
			"common",
			"shared",
			"packages",
			"typescript",
			"better-auth"
		]
	},
	"dependencies": {
		"effect": "^3.18.4",
		"better-auth": "^1.3.27",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"tslib": "^2.3.0",
		"vite-plugin-dts": "^4.5.4",
		"vite": "^7.0.0",
		"vitest": "^3.0.0",
		"@nx/vite": "21.6.5" 
	}
}
