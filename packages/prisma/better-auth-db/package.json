{
	"name": "@emperorrag/prisma-better-auth-db",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js",
			"@emperorrag/source": "./src/index.ts"
		},
		"./client": {
			"types": "./dist/lib/prisma/generated/client/index.d.ts",
			"require": "./dist/lib/prisma/generated/client/index.js",
			"import": "./dist/lib/prisma/generated/client/index.js",
			"default": "./dist/lib/prisma/generated/client/index.js"
		},
		"./schemas": {
			"types": "./dist/lib/prisma/generated/schemas/index.d.ts",
			"import": "./dist/lib/prisma/generated/schemas/index.js",
			"default": "./dist/lib/prisma/generated/schemas/index.js"
		},
		"./types": {
			"types": "./dist/lib/prisma/generated/schemas/index.d.ts",
			"import": "./dist/lib/prisma/generated/schemas/index.js",
			"default": "./dist/lib/prisma/generated/schemas/index.js"
		}
	},
	"files": [
		"README.md",
		"scripts",
		"dist",
		"!**/*.tsbuildinfo"
	],
	"nx": {
		"name": "prisma-better-auth-db",
		"targets": {
			"prisma-generate": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma generate --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			},
			"prisma-migrate": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma migrate dev --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			},
			"prisma-migrate-deploy": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma migrate deploy --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			},
			"prisma-studio": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma studio --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			},
			"prisma-push": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma db push --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			},
			"prisma-reset": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma migrate reset --schema=packages/prisma/better-auth-db/prisma/schema.prisma --force",
					"cwd": "."
				}
			},
			"prisma-validate": {
				"executor": "nx:run-commands",
				"cache": false,
				"options": {
					"command": "pnpm exec prisma validate --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
					"cwd": "."
				}
			}
		},
		"tags": [
			"prisma",
			"database",
			"shared"
		]
	},
	"dependencies": {
		"@prisma/client": "6.17.1",
		"better-auth": "^1.3.27",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"prisma": "^6.17.1",
		"tslib": "^2.3.0",
		"vite-plugin-dts": "^4.5.4",
		"vite": "^7.0.0",
		"vitest": "^3.0.0",
		"@nx/vite": "21.6.5",
		"@swc-node/register": "~1.9.1"
	}
}
