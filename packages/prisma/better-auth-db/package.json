  "files": [
    "README.md",
    "scripts",
    "dist",
    "src",
    "!src/lib/prisma/generated/**",
    "!**/*.tsbuildinfo",
    "!**/*.test.*",
    "!**/*.spec.*"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@emperorrag/prisma-better-auth-db",
  "nx": {
    "name": "prisma-better-auth-db",
    "tags": [
      "prisma",
      "database",
      "shared"
    ],
    "targets": {
      "prisma-generate": {
        "cache": true,
        "executor": "nx:run-commands",
        "inputs": [
          "{projectRoot}/prisma/schema.prisma",
          "{projectRoot}/prisma/migrations/**/*"
        ],
        "options": {
          "command": "npx prisma generate --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        },
        "outputs": [
          "{projectRoot}/prisma/generated"
        ]
      },
      "prisma-migrate": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma migrate dev --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        }
      },
      "prisma-migrate-deploy": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma migrate deploy --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        }
      },
      "prisma-push": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma db push --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        }
      },
      "prisma-reset": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma migrate reset --schema=packages/prisma/better-auth-db/prisma/schema.prisma --force",
          "cwd": "."
        }
      },
      "prisma-studio": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma studio --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        }
      },
      "prisma-validate": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma validate --schema=packages/prisma/better-auth-db/prisma/schema.prisma",
          "cwd": "."
        }
      }
    }
  },
  "private": true,
  "scripts": {
    "clean:all": "npm run clean:dist && npm run clean:node_modules && npm run clean:out-tsc",
    "clean:dist": "npx rimraf dist",
    "clean:node_modules": "npx rimraf node_modules",
    "clean:out-tsc": "npx rimraf out-tsc",
    "generate-graph": "madge --extensions ts --include-npm --json src > dependency-graph.json && madge --extensions ts --include-npm --image dependency-graph.svg src",
    "tsc:list-files": "tsc --project tsconfig.json --listFiles",
    "tsc:list-files:lib": "tsc --project tsconfig.lib.json --listFiles",
    "tsc:list-files:spec": "tsc --project tsconfig.spec.json --listFiles"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.1"
}
