  "search.maxResults": 100,
  "search.quickOpen.includeSymbols": true,
  "search.smartCase": true,
  "terminal.integrated.automationProfile.windows": {
    "path": "C:\\Program Files\\Git\\bin\\bash.exe"
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorStyleInactive": "none",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "always",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.environmentChangesIndicator": "on",
  "terminal.integrated.environmentChangesRelaunch": true,
  "terminal.integrated.fontFamily": "Fira Code, monospace",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "color": "terminal.ansiGreen",
      "icon": "github",
      "source": "Git Bash"
    },
    "PowerShell": {
      "color": "terminal.ansiCyan",
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.suggest.windowsExecutableExtensions": {
    "com": false,
    "msi": false,
    "pl": false,
    "rb": false,
    "vbs": false
  },
  "terminal.integrated.tabs.showActions": "always",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.windowsUseConptyDll": true,
  "testing.showAllMessages": true,
  "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
  "todo-tree.tree.disableCompactFolders": true,
  "todo-tree.tree.showBadges": true,
  "todo-tree.tree.showCountsInTree": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.autoImportFileExcludePatterns": [
    "**/dist/**",
    "**/out-tsc/**",
    "**/coverage/**",
    "**/prisma/generated/**"
  ],
  "typescript.tsserver.watchOptions": {
    "excludeDirectories": [
      "**/dist",
      "**/out-tsc",
      "**/node_modules",
      "**/.nx",
      "**/.git",
      "**/.next",
      "**/.pnpm-store",
      "**/coverage",
      "**/prisma/generated",
      "**/backups",
      "**/init-scripts",
      "**/scripts"
    ],
    "fallbackPolling": "dynamicPriorityPolling",
    "watchDirectory": "useFsEvents"
  },
  "vitest.configSearchPatternExclude": "{**/node_modules/**,**/.*/**,**/*.d.ts,**/dist/**,**/out-tsc/**,**/coverage/**}",
  "vitest.configSearchPatternInclude": "**/*vitest.config*.{ts,js,mjs,cjs,cts,mts}",
  "vitest.filesWatcherInclude": "{apps,packages}/**/*.{ts,tsx,js,jsx,json}",
  "vitest.vitestPackagePath": "node_modules/vitest/package.json",
  "vitest.workspaceConfig": "vitest.workspace.ts",
  "wallaby.codeLensFeature.runTest": true,
  "wallaby.compactStatus": false,
  "wallaby.reportFailuresAsProblems": false,
  "workbench.colorCustomizations": {
    "[Monokai]": {
      "editor.background": "#000000"
    }
  },
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.wrapTabs": true,
  "yaml.format.printWidth": 300
}