	"nx": {
		"includedScripts": [],
		"targets": {
			"local-registry": {
				"executor": "@nx/js:verdaccio",
				"options": {
					"port": 4873,
					"config": ".verdaccio/config.yml",
					"storage": "tmp/local-registry/storage"
				}
			},
			"test-all-projects": {
				"executor": "nx:run-commands",
				"options": {
					"command": "nx run-many -t test --all --verbose",
					"parallel": false
				}
			},
			"patch-tslib-for-effect": {
				"executor": "nx:run-commands",
				"options": {
					"command": "npx effect-language-service patch"
				}
			}
		}
	}
